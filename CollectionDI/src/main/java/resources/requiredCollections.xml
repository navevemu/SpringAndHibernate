<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:util="http://www.springframework.org/schema/util"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	 http://www.springframework.org/schema/util
                    http://www.springframework.org/schema/util/spring-util-2.5.xsd">
	
	<bean id="dc" class="beans.RequiredCollections">
	<property name="fruits">
	<util:list list-class="java.util.Vector">
	<value>Apple</value>
	<value>Mango</value>
	<value>Grape</value>
	<value>Guava</value>
	<value>Orange</value>
	
	</util:list>
		</property>
<property name="vegitables">
<util:set set-class="java.util.HashSet">

<value>Cocumber</value>
	<value>Carrot</value>
	<value>Chilli</value>
	<value>Capsicum</value>
	<value>Tomato</value>
</util:set>
</property>	
<property name="cc">
<util:map map-class="java.util.Hashtable">
<entry key="india" value="Delhi"></entry>
<entry key="USA" value="NewYork"></entry>
<entry key="UK" value="London"></entry>
<entry key="Belgium" value="Brussels"></entry>

</util:map>


</property>	
	</bean>
	</beans>